{
  "name": "with-typescript",
  "version": "1.0.0",
  "homepage": "https://digmovie.netlify.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomneo2004/MovieReviews"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "scripts": {
    "test": "jest",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format-code": "prettier --write \"src/**/*.{js,jsx,css,ts,tsx}\"",
    "commit": "cz",
    "e2e-test": "server-test dev :3000 cy-open",
    "ci-test": "server-test dev :3000 cy-run",
    "ci-test-record": "server-test dev :3000 cy-run-record",
    "cy-open": "cypress open",
    "cy-run": "cypress run",
    "cy-run-record": "cypress run --record",
    "netlify-build": "next-on-netlify"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@types/url-join": "^4.0.0",
    "axios": "^0.20.0",
    "clsx": "^1.1.1",
    "framer-motion": "^3.3.0-beta.22",
    "moment": "^2.29.1",
    "next": "latest",
    "next-on-netlify": "^2.8.7",
    "nprogress": "^0.2.0",
    "react": "^16.12.0",
    "react-bottom-scroll-listener": "^5.0.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^5.0.3",
    "react-material-ui-carousel": "^2.0.1",
    "react-player": "^2.8.2",
    "remark-gfm": "^1.0.0",
    "sentence-tokenizer": "^1.0.1",
    "short-uuid": "^4.1.0",
    "shortid": "^2.2.16",
    "swr": "^0.3.6",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-amd": "^7.12.1",
    "@cypress/code-coverage": "^3.8.6",
